<%- include('parts/html-head') %>
<%- include('parts/navbar') %>

<div class="container">
    <div class="row">
        <div class="col">

            <table class="table table-striped table-bordered">
                <thead>
                  <tr>
                    <th scope="col">#</th>
                    <th scope="col">客戶序號</th>
                    <th scope="col">金額</th>
                    <th scope="col">訂單時間</th>
                    
                  </tr>
                </thead>
                <tbody>
                  <% for(let i of rs){ %>
                  <tr>
                    <td><%= i.sid %></td>
                    <td><%= i.member_sid %></td>
                    <td><%= i.amount %></td>
                    <td><%= toDateTimeString(i.order_date) %></td>
                   

                  </tr>
                  <% } %>
                </tbody>
            </table>
        </div>
    </div>



</div>

<%- include('parts/scripts') %>
<%- include('parts/html-foot') %>